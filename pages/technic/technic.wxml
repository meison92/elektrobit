<!--pages/technic/technic.wxml-->
<view>
	<view class="tech-nav">
		<view bindtap="tapNav" data-index="0" class="f16 {{techIndex==0?'green':''}}">Webinar</view>
		<view bindtap="tapNav" data-index="1" class="f16 {{techIndex==1?'green':''}}">技术文章</view>
		<view bindtap="tapNav" data-index="2" class="f16 {{techIndex==2?'green':''}}">白皮书</view>
	</view>
	<view class="line" />
	<view class="tech-container">
		<block
		 wx:if="{{techIndex==0}}"
		 wx:for="{{webinarList}}"
		 wx:for-item="item"
		 wx:key="item.id"
		>
			<webinar-item data="{{item}}" />
		</block>
		<view wx:if="{{techIndex==1}}">
			<view>
				<view class="green f14">范围广微积分v模式密封胶而网购骗局我没玩；方可为</view>
				<view class="grey1 f12">产品：Vsdwe343</view>
				<view class="grey1 f12">产品：Vsdwe343</view>
			</view>
		</view>
		<view wx:if="{{techIndex==2}}">
			<view>
				<view>白皮书</view>
			</view>
		</view>
	</view>
</view>

